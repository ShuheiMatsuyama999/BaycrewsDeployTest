@charset "utf-8";


/* Selectbox Styles
=================================*/

a.sctble_display {
	width: 231px;
	border: none;
	padding-right: 19px;
	background: url(../images/common/select_right.gif) no-repeat right top;
	-webkit-background-size: 19px 78px ;
}
a.sctble_display:hover { background-position: right -26px }
a.sctble_focus { background-position: right -52px !important }

	a.sctble_display span {
		padding-left: 7px;
		height: 26px;
		background: url(../images/common/select_left.gif) no-repeat left top;
		-webkit-background-size: 316px 78px ;
		line-height: 26px;
		color: #333;
		font-weight: bold;
	}
	a.sctble_display:hover span { background-position: left -26px }
	a.sctble_focus span { background-position: left -52px !important }

/* Pulldown Mat (Basis)
=================================*/

div.sctble_mat {
	background: #e1ead6;
}

	div.sctble_mat dl { 
		border-color: #93af72;
	}
	
	div.sctble_mat a {
		color: #262626;
	}
	
	div.sctble_mat a.selected {
		color: #666;
	}
	
/* Pulldown Mat (No Optgroups)
=================================*/
	
div.sctble_mat.nooptg p {
	border-color: solid #93af72 1px;
}

/* Date (Inline)
=================================*/

div.sctble_mat.sctble.m_year p,
div.sctble_mat.sctble.m_month p {}
		
	div.sctble_mat.sctble.m_year a,
	div.sctble_mat.sctble.m_month a {}
	
	div.sctble_mat.sctble.m_day a {}
		
/* Simple Select Box
=================================*/
	
div.sBox {
	width: 250px;
	border: solid #fff 1px;
	background: #fff;
}

div.sBox p {
	border: none !important;
}
	
	div.sBox a:hover,
	div.sBox a:focus,
	div.sBox a.selected {
		color: #333;
		background: #e6e6e6;
	}


